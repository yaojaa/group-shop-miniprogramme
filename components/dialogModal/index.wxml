<view class="container" wx:if="{{isShow}}" catchtouchmove="preventTouchMove">
  <view class="back-model"></view>
  <view class="conent-model">
    <text class="title">{{title}}</text>
    <text class="content">{{content}}</text>
    <view class="quickBtn">
      <button class="btn-reset cancel-btn" open-type="{{cancelType}}" bindtap="cancel">
        {{cancelText}}
      </button>
      <button class="btn-reset confirm-btn" open-type="{{confirmType}}" bindtap="confirm">
        {{confirmText}}
      </button>
    </view>
  </view>
</view>