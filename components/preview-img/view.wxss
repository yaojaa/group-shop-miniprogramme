/*蒙版*/
.swiper{
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 99999;
	background-color: #000;
}
.slide-image{
	width: 100%;
	height: 100%;
}
.mask{
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	z-index: 999999;
	background: #000;
	opacity: 0.4;
}
.save-img{
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 60px;
	z-index: 9999999;
	background: #fff;
	font-size: 18px;
	line-height: 60px;
	text-align: center;
}