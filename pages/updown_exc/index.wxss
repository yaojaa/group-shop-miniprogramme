.view-page{
	background-color:#fff;
}
.page-title{
  font-size: 20px;
  line-height: 36px;
  padding: 20px 0 40px;
  text-align: center;
  letter-spacing: 1px;
}
.exc-btn-list .btn-cell{
  margin: 0 2px;
}
.exc-btn-list{
  font-size: 14px;
}
.exc-select-time{
  display: flex;
  font-size: 16px;
  line-height: 46px;
  border-bottom: 1px solid #eee;
  margin: 0 14px;
}
.exc-select-time .left{
  width: 75px;
  flex-shrink: 0;
}
.exc-select-time .right{
  flex-shrink: 0;
  flex-grow: 1;
  display: flex;
  align-items: center;
  font-size: 14px;
  color: #666;
}

i-datepicker{
  width: 100%;
  height: 100%;
}

.i-picker-title{
  text-align: right;
}

.create-link{
  padding-top: 160px;
  text-align: center;
}

.create-link .btn{
  font-size: 16px;
  letter-spacing: 1px;
  width: 60%;
}