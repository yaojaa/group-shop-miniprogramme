page{
   background-color: #fcfffa; 
}


.background image{
  width: 100%;
  height: 100%;
}
.content{
  height: 100%;
  padding: 0 15px 200rpx;
}
.header{
 align-items: center;
  text-align: center;
  display: block;
  padding: 30rpx
}
.header .title{
  font-size: 24px;
  color: green;
  font-weight:bold
}

.order .title{
  font-size: 32rpx;
  line-height: 1.7;
  margin:0  20px;
  font-weight: bold;
  border-bottom: 1px dotted #ccc;
  padding: 8rpx 0
}
.line{
  font-size: 14px;
  border-top: 1px dotted #ccc;
  margin: 20px;
  padding: 20px
}
.line::before
,.line::after{
  position: absolute;
  top: -7px;
  width: 12px;
  height: 12px;
  background-color: #fff;
  content: "";
  border-radius: 50%;
}
.line::before{
  left: -26px;
}
.line::after{
  right: -26px;
}

.van-checkbox__label {
  color: #333;
  font-size: 14px;
  line-height: 1.2;

}

