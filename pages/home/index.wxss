.line {
    position: relative;
    height: 0;
    font-size: 0;
    border-top: 1px dotted #ccc;
}

.line::before,
.line::after {
    position: absolute;
    top: -7px;
    width: 12px;
    height: 12px;
    background-color: #fff;
    content: "";
    border-radius: 50%;
}

.line::before {
    left: -26px;
}

.line::after {
    right: -26px;
}

