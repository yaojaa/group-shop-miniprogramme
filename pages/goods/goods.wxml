<view class="container">
  <view class="page-section page-section-spacing swiper">
    <swiper indicator-dots="{{true}}" autoplay="{{true}}" class="swiper-primary">
      <block wx:for="{{imgUrls}}" wx:key="{{index}}">
        <swiper-item>
          <image src="{{item}}" class="slide-image" />
        </swiper-item>
      </block>
    </swiper>
  </view>
</view>