    <i-panel class="cell-panel-demo" title="有{{user_count}}人 {{total}}次访问">
            <block wx:for="{{list}}" wx:for-item="listItem">
            <view wx:for="{{listItem}}" class="guest-box guest-item">
                    <view class="hd">
                        <image class="avatar" src="{{item.headimg}}" mode="cover"></image> 
                    </view>
                    <view class="bd">
                        <view class="text-justify">
                            <view class="name">{{item.nickname}} <text style="font-size: 12px;color: #999"> ({{item.user_phone}}) </text> </view>
                            <view class="info">
                                <view class="num">
                                    
                                </view>
                                <view class="num">
                                </view>
                            </view>
                        </view>
                        <view class="date">
                           {{item.time_ago}}   <!-- 使用{{item.user_phone}} 手机  -->浏览了{{item.user_staytime}} 
                         <!-- {{secen[item.user_scene]}}打开， -->
                        </view>
                    </view>
            </view>
            </block>
    </i-panel>
</view>