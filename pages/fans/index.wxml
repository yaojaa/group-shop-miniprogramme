                    <view class="hd">
                        <image class="avatar" src="{{item.headimg}}" mode="cover"></image>
                    </view>
                    <view class="bd">
                        <view class="text-justify">
                            <view class="name">{{item.nickname}}</view>
                            <view class="info">
                                <view class="num">
                                    
                                </view>
                                <view class="num">
                                    已参团
